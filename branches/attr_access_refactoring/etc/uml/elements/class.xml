<ElementType xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd" id="Class">
    <Icon path="icons/class.png"/>
    <Domain id="class"/>
    <Connections>
        <Item value="Association"/>
        <Item value="Generalization" with="Class" allowrecursive="0"/>
        <Item value="Agregation" with="Class" allowrecursive="1"/>
        <Item value="Compose" with="Class" allowrecursive="0"/>
    </Connections>
    <Appearance>
        <Shadow padding="3" color="#cfg.Styles.Element.ShadowColor">
            <Rectangle fill="#cfg.Styles.Element.FillColor" border="#cfg.Styles.Element.LineColor">
                <HBox>
                    <Padding padding="5">
                        <HBox>
                            <Condition condition="#self.stereotype" type="empty" negate="1">
                                <Align align="center">
                                    <VBox>
                                        <TextBox text="&#171;" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                        <TextBox text="#self.stereotype" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                        <TextBox text="&#187;" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                    </VBox>
                                </Align>
                            </Condition>
                            <Align align="center">
                                <TextBox text="#self.name" font="#cfg.Styles.Element.NameTextFont" color="#cfg.Styles.Element.NameTextColor"/>
                            </Align>
                        </HBox>
                    </Padding>
                    <Line type="horizontal" color="#cfg.Styles.Element.LineColor"/>
                    <Padding padding="5">
                        <HBox>
                            <Loop collection="#self.attributes">
                                <Condition type="empty" condition="#name" negate="1">
                                    <VBox>
                                        <Switch value="#scope">
                                            <Case condition="Private">
                                                <TextBox text=" - " font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                            </Case>
                                            <Case condition="Protected">
                                                <TextBox text=" # " font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                            </Case>
                                            <Case condition="Public">
                                                <TextBox text=" + " font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                            </Case>
                                        </Switch>
                                        <Condition type="empty" condition="#stereotype" negate="1">
                                            <VBox>
                                                <TextBox text="&#171;" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                <TextBox text="#stereotype" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                <TextBox text="&#187;  " font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                            </VBox>
                                        </Condition>
                                        <TextBox text="#name" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                        <Condition type="empty" condition="#type" negate="1">
                                            <VBox>
                                                <TextBox text=": " font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                <TextBox text="#type" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                            </VBox>
                                        </Condition>
                                    </VBox>
                                </Condition>
                            </Loop>
                        </HBox>
                    </Padding>
                    <Condition condition="#self.operations" type="empty" negate="1">
                        <HBox>
                            <Line type="horizontal" color="#cfg.Styles.Element.LineColor"/>
                            <Padding padding="5">
                                <HBox>
                                    <Loop collection="#self.operations">
                                        <Condition type="empty" condition="#name" negate="1">
                                            <VBox>
                                                <Switch value="#scope">
                                                    <Case condition="Private">
                                                        <TextBox text=" - " font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                    </Case>
                                                    <Case condition="Protected">
                                                        <TextBox text=" # " font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                    </Case>
                                                    <Case condition="Public">
                                                        <TextBox text=" + " font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                    </Case>
                                                </Switch>
                                                <Condition type="empty" condition="#stereotype" negate="1">
                                                    <VBox>
                                                        <TextBox text="&#171;" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                        <TextBox text="#stereotype" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                        <TextBox text="&#187;  " font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                    </VBox>
                                                </Condition>
                                                <TextBox text="#name" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                <TextBox text="(" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                <Loop collection="#parameters">
                                                    <VBox>
                                                        <Condition type="empty" condition="#type" negate="1">
                                                            <VBox>
                                                                <Condition type="equal" condition="#_line" value="0" negate="1">
                                                                    <TextBox text=", " font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                </Condition>
                                                                <Condition type="equal" condition="#const" value="True">
                                                                    <TextBox text="const " font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                </Condition>
                                                                <Condition type="equal" condition="#scope" value="in" negate="1">
                                                                    <VBox>
                                                                        <TextBox text="[" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                        <TextBox text="#scope" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                        <TextBox text="] " font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                    </VBox>
                                                                </Condition>
                                                                <Condition type="empty" condition="#name" negate="1">
                                                                    <VBox>
                                                                        <TextBox text="#name" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                    </VBox>
                                                                </Condition>
                                                                <Condition type="empty" condition="#type" negate="1">
                                                                    <VBox>
                                                                        <TextBox text=":" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                        <Condition type="empty" condition="#name" negate="1">
                                                                            <TextBox text=" " font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                        </Condition>
                                                                        <TextBox text="#type" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                    </VBox>
                                                                </Condition>
                                                                <Condition type="empty" condition="#default" negate="1">
                                                                    <VBox>
                                                                        <TextBox text=" = " font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                        <TextBox text="#default" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                    </VBox>
                                                                </Condition>
                                                            </VBox>
                                                        </Condition>
                                                        <Condition type="empty" condition="#type">
                                                            <Condition type="empty" condition="#name" negate="1">
                                                                <VBox>
                                                                    <Condition type="equal" condition="#_line" value="0" negate="1">
                                                                        <TextBox text=", " font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                    </Condition>
                                                                    <Condition type="equal" condition="#const" value="True">
                                                                        <TextBox text="const " font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                    </Condition>
                                                                    <Condition type="equal" condition="#scope" value="in" negate="1">
                                                                        <VBox>
                                                                            <TextBox text="[" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                            <TextBox text="#scope" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                            <TextBox text="] " font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                        </VBox>
                                                                    </Condition>
                                                                    <Condition type="empty" condition="#name" negate="1">
                                                                        <VBox>
                                                                            <TextBox text="#name" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                        </VBox>
                                                                    </Condition>
                                                                    <Condition type="empty" condition="#type" negate="1">
                                                                        <VBox>
                                                                            <TextBox text=":" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                            <Condition type="empty" condition="#name" negate="1">
                                                                                <TextBox text=" " font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                            </Condition>
                                                                            <TextBox text="#type" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                        </VBox>
                                                                    </Condition>
                                                                    <Condition type="empty" condition="#default" negate="1">
                                                                        <VBox>
                                                                            <TextBox text=" = " font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                            <TextBox text="#default" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                                        </VBox>
                                                                    </Condition>
                                                                </VBox>
                                                            </Condition>
                                                        </Condition>
                                                    </VBox>
                                                </Loop>
                                                <TextBox text=")" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                <Condition type="empty" condition="#rtype" negate="1">
                                                    <VBox>
                                                        <TextBox text=": " font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                        <TextBox text="#rtype" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                                                    </VBox>
                                                </Condition>
                                            </VBox>
                                        </Condition>
                                    </Loop>
                                </HBox>
                            </Padding>
                        </HBox>
                    </Condition>
                </HBox>
            </Rectangle>
        </Shadow>
    </Appearance>
</ElementType>