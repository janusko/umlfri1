<ElementType xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd" id="VerticalSynchronization">
    <Icon path="icons/vertical_line.png"/>
    <Domain id="synchronization"/>
    <Connections>
        <Item value="StateTransition"/>
        <Item value="ActivityFlow"/>
    </Connections>
    <Appearance>
        <VBox>
            <Sizer width="5" minheight="50">
                <Rectangle fill="#cfg.Styles.Element.Fill3Color" border="#cfg.Styles.Element.LineColor"/>
            </Sizer>
            <Condition condition="#self.name" type="empty" negate="1">
                <Padding padding="10">
                    <Align align="center center">
                        <TextBox text="#self.name" font="#cfg.Styles.Element.NameTextFont" color="#cfg.Styles.Element.NameTextColor"/>
                    </Align>
                </Padding>
            </Condition>
        </VBox>
    </Appearance>
    <Options>
    </Options>
</ElementType>