<ElementType xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd" id="Note">
    <Icon path="icons/note.png"/>
    <Domain id="note"/>
    <Connections>
        <Item value="Note Link" with="*"/>
    </Connections>
    <Appearance>
        <Shadow padding="3" color="#cfg.Styles.Element.ShadowColor">
            <Rectangle fill="#cfg.Styles.Element.FillColor" border="#cfg.Styles.Element.LineColor" righttop="10 /Styles/Element/Fill2Color note">
                <Sizer minwidth="50">
                    <Padding padding="5">
                        <TextBox text="#self.note" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                    </Padding>
                </Sizer>
            </Rectangle>
        </Shadow>
    </Appearance>
    <Options>
    </Options>
</ElementType>