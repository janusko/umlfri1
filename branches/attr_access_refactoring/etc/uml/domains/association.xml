<Domain id="association" xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd" >
    <Attribute id="name" type="str" name="Name" />
    <Attribute id="direction" type="enum" name="Direction">
        <Enum>Unspecified</Enum>
        <Enum>Source to Destination</Enum>
        <Enum>Destination to Source</Enum>
        <Enum>Bidirectional</Enum>
    </Attribute>
    <Attribute id="stereotype" type="str" name="Stereotype" />
    <Attribute id="SCardinality" type="str" name="Source Cardinality" />
    <Attribute id="DCardinality" type="str" name="Destination Cardinality" />
    <Attribute id="SRole" type="str" name="Source Role" />
    <Attribute id="DRole" type="str" name="Destination Role" />
    <Attribute id="note" type="text" name="Notes" />
</Domain>