<?xml version="1.0" encoding="utf-8"?>
<ConnectionType id="Generalization" xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd">
    <Icon path="icons/generalize.png" />
    <SrcArrow possible="0" default="0" />
    <DestArrow possible="1" default="1" />
    <Domain id="generalization" identity="name" />
    <Appearance>
        <LineStyle style="solid" color="#cfg.Styles.Connection.LineColor"/>
        <ArrowStyle style="triangle" fill="#cfg.Styles.Connection.ArrowFillColor" color="#cfg.Styles.Connection.ArrowColor"/>
        <Label position="center">
            <Condition condition="#self.name" type="empty" negate="1">
                <HBox>
                    <Condition condition="#self.stereotype" type="empty" negate="1">
                        <Align align="center">
                            <Rectangle fill="#cfg.Styles.Connection.TextFill">
                                <VBox>
                                    <TextBox text="&#xAB;" font="#cfg.Styles.Connection.TextFont" color="#cfg.Styles.Connection.TextColor"/>
                                    <TextBox text="#self.stereotype" font="#cfg.Styles.Connection.TextFont" color="#cfg.Styles.Connection.TextColor"/>
                                    <TextBox text="&#xBB;" font="#cfg.Styles.Connection.TextFont" color="#cfg.Styles.Connection.TextColor"/>
                                </VBox>
                            </Rectangle>
                        </Align>
                    </Condition>
                    <Condition condition="#self.name" type="empty" negate="1">
                        <Align align="center">
                            <Rectangle fill="#cfg.Styles.Connection.TextFill">
                                <TextBox text="#self.name" font="#cfg.Styles.Connection.NameTextFont" color="#cfg.Styles.Connection.NameTextColor"/>
                            </Rectangle>
                        </Align>
                    </Condition>
                </HBox>
            </Condition>
        </Label>
    </Appearance>
</ConnectionType>