<?xml version="1.0" ?>
<interface
    xmlns="http://umlfri.org/xmlschema/publicApi.xsd"
    name="org.umlfri.api.base.Template">
    
    <documentation>
        Information about project template.
    </documentation>
    
    <property name="name">
        <documentation>
            Project template name.
        </documentation>
        
        <value type="string" readable="true" />
    </property>
    
    <property name="metamodelUri">
        <documentation>
            Metamodel associated with the project template.
        </documentation>
        
        <value type="string" readable="true"/>
    </property>
    
    <method name="createNewProject" mutator="true" transactional="false">
        <documentation>
            Create new project using the template.
        </documentation>
    </method>
</interface>
