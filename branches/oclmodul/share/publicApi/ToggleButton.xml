<?xml version="1.0" ?>
<interface
    xmlns="http://umlfri.org/xmlschema/publicApi.xsd"
    name="org.umlfri.api.gui.ToggleButton"
    base="org.umlfri.api.gui.Button">
    
    <documentation>
        Simple two-state toggle button.
    </documentation>
    
    <property name="active">
        <documentation>
            Contains (toggled) state of button.
        </documentation>
        <value type="boolean" readable="true" writable="true" />
    </property>
</interface>
