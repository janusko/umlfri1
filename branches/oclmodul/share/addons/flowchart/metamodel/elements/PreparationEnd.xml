<ElementType id="Preparation End" xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd">
    <Icon path="icons/PreparationEnd.png"/>
    <Domain id="PreparationEnd" identity="name"/>
    <Connections>
        <Item value="Note Link" with="Note"/>
        <Item value="Flow Line"/>
    </Connections>
    <Appearance>
        <Shadow padding="3" color="#cfg.Styles.Element.ShadowColor">
            <Rectangle fill="#cfg.Styles.Element.FillColor" border="#cfg.Styles.Element.LineColor" right="10 beak_side" left="10 beak_side">
                <Sizer minheight="30" minwidth="100">
                    <Padding padding="10"> 
                        <Align align="center center">                        
                            <TextBox text="#self.variable" font="#cfg.Styles.Element.NameTextFont" color="#cfg.Styles.Element.NameTextColor"/>                         
                        </Align>
                    </Padding>
                </Sizer>
            </Rectangle>
        </Shadow>
    </Appearance>
    <Options>
    </Options>
</ElementType>