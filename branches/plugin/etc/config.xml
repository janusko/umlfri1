<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Config [<!ENTITY apppath "&#255;">]>
<Config xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/config.xsd">
    <Styles>
        <Element>
            <LineColor>black</LineColor>
            <FillColor>lightyellow</FillColor>
            <Fill2Color>lightyellow</Fill2Color>
            <Fill3Color>black</Fill3Color>
            <ShadowColor>#505050</ShadowColor>
            <NameTextColor>black</NameTextColor>
            <TextColor>black</TextColor>
            <NameTextFont>Arial 10</NameTextFont>
            <TextFont>Arial 10</TextFont>
        </Element>
        <Connection>
            <LineColor>black</LineColor>
            <ArrowColor>black</ArrowColor>
            <ArrowFillColor>white</ArrowFillColor>
            <ArrowAngleSteps>128</ArrowAngleSteps>
            <NameTextColor>black</NameTextColor>
            <TextColor>black</TextColor>
            <NameTextFont>Arial 10</NameTextFont>
            <TextFont>Arial 10</TextFont>
            <TextFill>white</TextFill>
            <MinimalAngle>0.1</MinimalAngle>
