        <UserDir>~/.uml_fri</UserDir>
        <UserConfig>~/.uml_fri/config.xml</UserConfig>
        <UserAddons>~/.uml_fri/addons/</UserAddons>
        <UserTemplates>~/.uml_fri/templates/</UserTemplates>
        <UserEnabledAddons>~/.uml_fri/addons.xml</UserEnabledAddons>
        <RecentFiles>~/.uml_fri/RecentFiles.xml</RecentFiles>
        <UserGui>~/.uml_fri/UserGui.xml</UserGui>
        <Plugins>&apppath;/plugins</Plugins>
    </Paths>
    <Page>
        <Width>797</Width>
        <Height>1123</Height>
    </Page>
</Config>
