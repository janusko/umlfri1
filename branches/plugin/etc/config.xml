        <Root>&apppath;</Root>
        <Addons>&apppath;/share/addons/</Addons>
        <Templates>&apppath;/etc/templates/</Templates>
        <Images>&apppath;/img/</Images>
        <Gui>&apppath;/gui/</Gui>
