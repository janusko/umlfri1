<?xml version="1.0" encoding="utf-8"?>
<ConnectionType id="Generalization" xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd">
    <Icon path="icons/generalize.png" />
    <SrcArrow possible="0" default="0" />
    <DestArrow possible="1" default="1" />
    <Domain id="generalization" />
    <Appearance>
        <LineStyle style="solid" color="/Styles/Connection/LineColor" />
        <ArrowStyle style="triangle" fill="/Styles/Connection/ArrowFillColor" color="/Styles/Connection/ArrowColor" />
        <Label position="center">
            <Condition condition="#name" type="empty" negate="1">
                <HBox>
                    <Condition condition="#stereotype" type="empty" negate="1">
                        <Align align="center">
                            <Rectangle fill="/Styles/Connection/TextFill">
                                <VBox>
                                    <TextBox text="&#171;" font="/Styles/Connection/TextFont" color="/Styles/Connection/TextColor" />
                                    <TextBox text="#stereotype" font="/Styles/Connection/TextFont" color="/Styles/Connection/TextColor" />
                                    <TextBox text="&#187;" font="/Styles/Connection/TextFont" color="/Styles/Connection/TextColor" />
                                </VBox>
                            </Rectangle>
                        </Align>
                    </Condition>
                    <Condition condition="#name" type="empty" negate="1">
                        <Align align="center">
                            <Rectangle fill="/Styles/Connection/TextFill">
                                <TextBox text="#name" font="/Styles/Connection/NameTextFont" color="/Styles/Connection/NameTextColor" />
                            </Rectangle>
                        </Align>
                    </Condition>
                </HBox>
            </Condition>
        </Label>
    </Appearance>
</ConnectionType>
