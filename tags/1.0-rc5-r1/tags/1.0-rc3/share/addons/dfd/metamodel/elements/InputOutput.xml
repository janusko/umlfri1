<ElementType id="InputOutput" xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd">
    <Icon path="icons/InputOutput.png"/>
    <Domain id="InputOutput" identity="iocommand"/>
    <Connections>
        <Item value="NoteLink" with="Note"/>
        <Item value="FlowLine"/>
    </Connections>
    <Appearance>
        <Shadow padding="3" color="#cfg.Styles.Element.ShadowColor">
            <Rectangle fill="#cfg.Styles.Element.FillColor" border="#cfg.Styles.Element.LineColor">
                <Sizer minheight="50" minwidth="100">
                    <Padding padding="5">             
                        <Align align="center center">
                            <TextBox text="#self.iocommand" font="#cfg.Styles.Element.NameTextFont" color="#cfg.Styles.Element.NameTextColor"/>  
                        </Align>         
                    </Padding>    
                </Sizer>
            </Rectangle>
        </Shadow>
    </Appearance>
    <Options>
    </Options>
</ElementType>