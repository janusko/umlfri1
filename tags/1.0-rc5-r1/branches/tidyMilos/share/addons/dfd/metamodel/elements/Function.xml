<ElementType id="Function" xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd">
    <Icon path="icons/Function.png"/>
    <Domain id="Function" identity="functionname"/>
    <Connections>
        <Item value="NoteLink" with="Note"/>
        <Item value="FlowLine"/>
    </Connections>
    <Appearance>
        <Shadow padding="3" color="#cfg.Styles.Element.ShadowColor">
            <Ellipse fill="#cfg.Styles.Element.FillColor" border="#cfg.Styles.Element.LineColor">
                <Proportional ratio="1:1" align="center center">
                    <Padding padding="20">
                        <Align align="center center">
                            <TextBox text="#self.functionname" font="#cfg.Styles.Element.NameTextFont" color="#cfg.Styles.Element.NameTextColor"/>
                        </Align>
                    </Padding>
                </Proportional>
            </Ellipse>
        </Shadow>
    </Appearance>
    <Options>
    </Options>
</ElementType>