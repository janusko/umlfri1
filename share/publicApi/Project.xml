<?xml version="1.0" ?>
<interface
    xmlns="http://umlfri.org/xmlschema/publicApi.xsd"
    name="org.umlfri.api.model.Project">
    
    <documentation>
        Root object for project definition.
    </documentation>
    
    <property name="fileName">
        <documentation>
            Full path to the project file.
        </documentation>
        
        <value type="string" readable="true" />
    </property>
    
    <property name="root">
        <documentation>
            Project root element.
        </documentation>
        
        <value type="org.umlfri.api.model.ElementObject" readable="true" />
    </property>
    
    <property name="metamodel">
        <documentation>
            Metamodel associated with the project.
        </documentation>
        
        <value type="org.umlfri.api.metamodel.Metamodel" readable="true" />
    </property>

    <property name="values" singular="value">
        <documentation>
            Collection, that contains all property values.
        </documentation>

        <index name="path" type="string">
            <documentation>
                Identifier of the project attribute.
            </documentation>
        </index>
        <value type="variant" readable="true" writable="true" />
    </property>

    <property name="allValues">
        <documentation>
            Iterator over all property values.
        </documentation>

        <value type="keyvalue" iterable="true" />
    </property>


    <method name="modifyDomain" mutator="true">
        <documentation>
            Modify domain of the project.
        </documentation>

        <parameter name="modifications" type="string">
            <documentation>
                A list of domain attribute modifications.
            </documentation>
        </parameter>
    </method>

    
    <method name="save" mutator="true" transactional="false">
        <documentation>
            Saves project to the current location.
        </documentation>
    </method>
    
    <method name="saveAs" mutator="true" transactional="false">
        <documentation>
            Saves project to the given file.
        </documentation>
        
        <parameter name="fileName" type="string">
            <documentation>
                Absolute path to the project file.
            </documentation>
        </parameter>
        
        <parameter name="isZippedFile" type="string">
            <documentation>
                True, if file is .frip/.frit; false if file is .fripx/fritx.
            </documentation>
        </parameter>
    </method>
</interface>
