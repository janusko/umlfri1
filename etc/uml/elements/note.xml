<?xml version="1.0" encoding="utf-8"?>
<ElementType id="Note" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd ..\share\schema\metamodel.xsd" xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd">
    <Icon path="icons/note.png" />
    <Connections>
        <Item value="Note Link" with="*" />
    </Connections>
    <Attributes>
        <Item value="Name" type="str" propid="name" />
        <Item value="Notes" type="note" propid="note" />
    </Attributes>
    <Appearance>
        <Shadow padding="3" color="/Styles/Element/ShadowColor">
            <Rectangle fill="/Styles/Element/FillColor" border="/Styles/Element/LineColor" righttop="10 /Styles/Element/Fill2Color note">
                <Sizer minwidth="50">
                    <Padding padding="5">
                        <TextBox text="#note" font="/Styles/Element/TextFont" color="/Styles/Element/TextColor" />
                    </Padding>
                </Sizer>
            </Rectangle>
        </Shadow>
    </Appearance>
</ElementType>
