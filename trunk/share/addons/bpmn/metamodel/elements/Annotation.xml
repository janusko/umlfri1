<?xml version="1.0" encoding="utf-8"?>
<ElementType id="Annotation" xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd">
    <Icon path="icons/Annotation.png" />
    <Domain id="Annotation"/>
    <Connections>
        <Item value="Association" with="*"/>
    </Connections>
    <Appearance>
        <Shadow padding="3" color="##888888">
            <Rectangle fill="#cfg.Styles.Element.FillColor">
                <Padding padding="-1">
                <Sizer minwidth="120" minheight="75">
                    <VBox expand="1">
                        
                        
                        <HBox expand="1">
                            
                            <Sizer height="2" width="50">
                            <Padding padding="1">
                                <Line type="horizontal" color="black"/>
                            </Padding>    
                            </Sizer>
                            <Sizer width="2">
                                <Line type="vertical" color="black"/>
                            </Sizer>    
                            
                            <Sizer height="2" width="50">
                                <Line type="horizontal" color="black"/>
                            </Sizer>   
                            
                        </HBox>
                        <Padding padding="5">
                            <Align align="center center">
                                <TextBox text="#self.note" font="#cfg.Styles.Element.TextFont" color="#cfg.Styles.Element.TextColor"/>
                            </Align>
                        </Padding>
                    </VBox>
                </Sizer>
                </Padding>
            </Rectangle>
        </Shadow>
    </Appearance>
    <Options>
    </Options>
</ElementType>