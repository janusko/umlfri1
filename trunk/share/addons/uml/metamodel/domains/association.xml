<Domain id="association" xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd" >
    <Attribute id="name" name="Name" >
		<Str/>
	</Attribute>
    <Attribute id="direction" name="Direction" >
		<Enum>
        	<Enum_value>Unspecified</Enum_value>
        	<Enum_value>Source to Destination</Enum_value>
        	<Enum_value>Destination to Source</Enum_value>
        	<Enum_value>Bidirectional</Enum_value>
		</Enum>
    </Attribute>
    <Attribute id="stereotype" name="Stereotype" >
		<Str/>
	</Attribute>
    <Attribute id="SCardinality" name="Source Cardinality" >
        <Enum>
			<Enum_value> </Enum_value>
        	<Enum_value>*</Enum_value>
        	<Enum_value>0</Enum_value>
        	<Enum_value>0..*</Enum_value>
        	<Enum_value>0..1</Enum_value>
        	<Enum_value>1</Enum_value>
        	<Enum_value>1..</Enum_value>
        	<Enum_value>1..*</Enum_value>
		</Enum>
    </Attribute>
    <Attribute id="DCardinality" name="Destination Cardinality" >
        <Enum>
			<Enum_value> </Enum_value>
	        <Enum_value>*</Enum_value>
	        <Enum_value>0</Enum_value>
	        <Enum_value>0..*</Enum_value>
	        <Enum_value>0..1</Enum_value>
	        <Enum_value>1</Enum_value>
	        <Enum_value>1..</Enum_value>
	        <Enum_value>1..*</Enum_value>
		</Enum>
    </Attribute>
    <Attribute id="SRole" name="Source Role" >
		<Str/>
	</Attribute>
    <Attribute id="DRole" name="Destination Role" >
		<Str/>
	</Attribute>
    <Attribute id="notes" name="Notes" >
		<Text/>
	</Attribute>
</Domain>