<?xml version="1.0" ?>
<interface
    xmlns="http://umlfri.org/xmlschema/publicApi.xsd"
    name="org.umlfri.api.gui.Widget"
    abstract="true">
    
    <documentation>
        Base class for all widgets.
    </documentation>
    
    <property name="enabled">
        <documentation>
            True, if widget is enabled.
        </documentation>
        <value type="boolean" readable="true" writable="true" />
    </property>
    
    <property name="visible">
        <documentation>
            Widget's visibility. True, if widget is visible.
        </documentation>
        <value type="boolean" readable="true" writable="true" />
    </property>
    
    <property name="guiId">
        <documentation>
            Widget's unique identifier.
        </documentation>
        <value type="string" readable="true" />
    </property>
</interface>
