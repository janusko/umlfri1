<?xml version="1.0" encoding="utf-8"?>
<ElementType id="UseCase" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd ..\share\schema\metamodel.xsd" xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd">
    <Icon path="icons/usecase.png" />
    <Connections>
        <Item value="Association" />
        <Item value="Generalization" with="UseCase" allowrecursive="0" />
    </Connections>
    <Attributes>
        <Item value="Name" type="str" propid="name" />
        <Item value="Abstract" type="bool" />
        <Item value="Notes" type="note" />
    </Attributes>
    <Appearance>
        <Shadow padding="3" color="/Styles/Element/ShadowColor">
            <Ellipse fill="/Styles/Element/FillColor" border="/Styles/Element/LineColor">
                <Proportional ratio="3:1">
                    <Padding padding="8">
                        <Align align="center center">
                            <TextBox text="#name" font="/Styles/Element/NameTextFont" color="/Styles/Element/NameTextColor" />
                        </Align>
                    </Padding>
                </Proportional>
            </Ellipse>
        </Shadow>
    </Appearance>
</ElementType>
