<?xml version="1.0" encoding="utf-8"?>
<ElementType id="StartState" xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd">
    <Icon path="icons/initial.png" />
    <Connections>
        <Item value="StateTransition"/>
    </Connections>
    <Attributes>
        <Item value="Name" type="str" propid="name" notgenerate="true"/>
        <Item value="Stereotype" type="str" propid="stereotype" />
        <Item value="Notes" type="note"/>
    </Attributes>
    <Appearance>
        <HBox expand="0">
            <Proportional ratio="1:1" align="center center" size="minimal">
                <Ellipse fill="/Styles/Element/Fill3Color">
                    <Sizer width="20" height="20" />
                </Ellipse>
            </Proportional>
            <HBox>
                <Condition condition="#stereotype" type="empty" negate="1">
                    <Align align="center">
                        <VBox>
                            <TextBox text="&#171;" font="/Styles/Element/TextFont" color="/Styles/Element/TextColor" />
                            <TextBox text="#stereotype" font="/Styles/Element/TextFont" color="/Styles/Element/TextColor" />
                            <TextBox text="&#187;" font="/Styles/Element/TextFont" color="/Styles/Element/TextColor" />
                        </VBox>
                    </Align>
                </Condition>
                <Align align="center">
                    <TextBox text="#name" font="/Styles/Element/NameTextFont" color="/Styles/Element/NameTextColor" />
                </Align>
            </HBox>
        </HBox>
    </Appearance>
    <Options>
    </Options>
</ElementType>
