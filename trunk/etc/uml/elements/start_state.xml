<?xml version="1.0" encoding="utf-8"?>
<ElementType id="StartState" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd ..\share\schema\metamodel.xsd" xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd">
    <Icon path="icons/initial.png" />
    <Connections>
        <Item value="StateTransition"/>
    </Connections>
    <Attributes>
        <Item value="Name" type="str" propid="name" notgenerate="true"/>
        <Item value="Notes" type="note"/>
    </Attributes>
    <Appearance>
        <HBox expand="0">
            <Ellipse fill="/Styles/Element/Fill3Color">
                <Sizer width="20" height="20" />
            </Ellipse>
            <Align align="center center">
                <Condition condition="#name" type="empty" negate="1">
                    <TextBox text="#name" font="/Styles/Element/NameTextFont" color="/Styles/Element/NameTextColor" />
                </Condition>
            </Align>
        </HBox>
    </Appearance>
</ElementType>
